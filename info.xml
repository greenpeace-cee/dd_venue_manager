<?xml version="1.0"?>
<extension key="dd_venue_manager" type="module">
  <file>dd_venue_manager</file>
  <name>DD Venue Manager</name>
  <description>Direct Dialog Venue Management</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/dd_venue_manager</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/dd_venue_manager</url>
    <url desc="Support">https://github.com/greenpeace-cee/dd_venue_manager/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-05-04</releaseDate>
  <version>1.0-beta.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.57</ver>
  </compatibility>
  <requires>
    <ext>org.civicrm.search_kit</ext>
    <ext>org.civicrm.afform</ext>
    <ext>org.wikimedia.relationshipblock</ext>
    <ext>org.civicrm.contactlayout</ext>
    <ext>token_manager</ext>
  </requires>
  <comments/>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/DdVenueManager</namespace>
    <format>22.10.0</format>
    <angularModule>crmDdVenueManager</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
